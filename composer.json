{
    "name": "silverhand7/laravel-api-response-wrapper",
    "description": "Wrapper for API response in Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silverhand7\\LaravelApiResponseWrapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "silverhand7",
            "email": "refojunior5@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.28.0",
        "illuminate/http": "^10.28.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silverhand7\\LaravelApiResponseWrapper\\ApiResponseWrapperServiceProvider"
            ],
            "aliases": {
                "ApiResponseWrapper": "Silverhand7\\LaravelApiResponseWrapper\\Facades\\ApiResponseWrapper"
            }
        }
    }
}
